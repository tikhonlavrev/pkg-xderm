sni=$(grep sni= config.txt 2>/dev/null|awk -F "=" '{print $2}')
mode=$(grep mode= config.txt|awk -F "=" '{print $2}')
   if [ -f $(badvpn-tun2socks -version 2>/dev/null|awk '{print $1}'|awk "NR==1") ]; then
